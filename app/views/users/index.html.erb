<div class="container">
	<h1>Users List</h1>

	<div class="table-responsive">

		<table class="table table-hover">
			<tr>
				<td><b>Company Name</b></td>
				<td><b>Company Email</b></td>
			</tr>
			<% @users.each do |user| %>
			<tr>
				<td><%= link_to user.company_name, user_path(user.id) %></td>
				<td><%=user.company_email%></td>
			</tr>
			<%end%>
		</table>
	</div>
</div>